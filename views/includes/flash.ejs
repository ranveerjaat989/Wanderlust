<%if(success && success.length){%>
    <div class="alert alert-info col-md-6 alert-dismissible  offset-3" role="alert">
    <%=success%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <%}%>
  
  
  <%if(error && error.length){%>
    <div class="alert alert-warning alert-dismissible col-md-6 offset-3" role="alert">
    <%=error%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <%}%>
